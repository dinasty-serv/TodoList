{{ form_start(form) }}
<div class="form-group">
    <label for="InputEmail">{{ form_label(form.email) }}</label>
    {{ form_widget(form.email) }}
</div>
<div class="form-group">
    <label for="InputEmail">{{ form_label(form.username) }}</label>
    {{ form_widget(form.username) }}

</div>
<div class="form-group">
    <label for="InputEmail">{{ form_label(form.password.first) }}</label>
    {{ form_widget(form.password.first,{'attr':{'class':'form-control'}}) }}

</div>
<div class="form-group">
    <label for="InputEmail">{{ form_label(form.password.second) }}</label>

    {{ form_widget(form.password.second,{'attr':{'class':'form-control'}}) }}

</div>
<div class="form-group">
    <label for="InputEmail">{{ form_label(form.roles) }}</label>
    {{ form_widget(form.roles) }}

</div>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
